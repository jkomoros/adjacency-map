{
  "$ref": "#/definitions/JSONData",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "EdgeType": {
      "type": "string"
    },
    "EdgeValue": {
      "additionalProperties": {
        "anyOf": [
          {
            "type": "number"
          },
          {
            "$ref": "#/definitions/EdgeType"
          },
          {
            "$ref": "#/definitions/NodeID"
          }
        ]
      },
      "properties": {
        "ref": {
          "$ref": "#/definitions/NodeID"
        },
        "type": {
          "$ref": "#/definitions/EdgeType"
        }
      },
      "required": [
        "type"
      ],
      "type": "object"
    },
    "JSONData": {
      "additionalProperties": false,
      "properties": {
        "nodes": {
          "additionalProperties": {
            "$ref": "#/definitions/NodeData"
          },
          "type": "object"
        },
        "root": {
          "$ref": "#/definitions/NodeValues"
        },
        "types": {
          "additionalProperties": {
            "additionalProperties": {
              "type": "number"
            },
            "properties": {
              "value": {
                "$ref": "#/definitions/ValueDefinition"
              }
            },
            "required": [
              "value"
            ],
            "type": "object"
          },
          "type": "object"
        },
        "version": {
          "type": "number"
        }
      },
      "required": [
        "version",
        "types",
        "root",
        "nodes"
      ],
      "type": "object"
    },
    "NodeData": {
      "additionalProperties": false,
      "properties": {
        "description": {
          "type": "string"
        },
        "values": {
          "items": {
            "$ref": "#/definitions/EdgeValue"
          },
          "type": "array"
        }
      },
      "required": [
        "description"
      ],
      "type": "object"
    },
    "NodeID": {
      "type": "string"
    },
    "NodeValues": {
      "additionalProperties": {
        "type": "number"
      },
      "description": "The final computed values of all of the values for a node.",
      "type": "object"
    },
    "ValueDefinition": {
      "type": "number"
    }
  }
}
